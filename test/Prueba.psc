Proceso Run
  Definir a, b, c como Entero;
  Definir str1, str2, str3 como Texto;
  Leer str1, str2, str3;
  a <- convertirANumero(str1);
  b <- convertirANumero(str2);
  c <- convertirANumero(str3);
  Escribir "La suma de a + b es ", a+b;
  Escribir "La suma de 2*b + a es ", 2*b+a;
  Escribir "El valor c es ", c;
FinProceso
